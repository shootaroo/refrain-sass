{
  "name": "refrain-sass",
  "version": "0.0.1",
  "description": "sass with refrain.",
  "keywords": [
    "refrain",
    "refrainplugin",
    "sass"
  ],
  "homepage": "https://github.com/shootaroo/refrain-sass",
  "bugs": "https://github.com/shootaroo/refrain-sass/issues",
  "license": "MIT",
  "author": "shootaroo <shotaro.tsubouchi@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shootaroo/refrain-sass.git"
  },
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --source-maps-inline",
    "watch": "babel src --out-dir lib --source-maps-inline --watch",
    "lint": "jscs src/*.js test/*.js && jshint src/*.js test/*.js",
    "test": "npm run lint && npm run build && mocha --compilers js:espower-babel/guess test/*.js",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "dependencies": {
    "node-sass": "^3.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^4.7.0",
    "espower-babel": "^1.3.0",
    "husky": "^0.6.0",
    "jscs": "^1.11.0",
    "jshint": "^2.6.0",
    "mocha": "^2.2.0",
    "power-assert": "^0.10.0",
    "refrain": "^0.1.0"
  }
}
